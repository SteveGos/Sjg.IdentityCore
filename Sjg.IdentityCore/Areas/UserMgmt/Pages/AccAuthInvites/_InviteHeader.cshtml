@model Sjg.IdentityCore.Models.AccAuthInvite

<div class="row">
    <div class="col-12 col-md-4">
        <div class="form-group">
            <label asp-for="Email" class="control-label"></label>
            <input asp-for="Email" class="form-control" readonly />
        </div>
    </div>
    <div class="col-12 col-md-4">
        <label asp-for="DisplayName" class="control-label"></label>
        <input asp-for="DisplayName" class="form-control" readonly />
    </div>
    <div class="col-12 col-md-4">
        <label asp-for="ExpirationDateUtc" class="control-label"></label>
        <input asp-for="ExpirationDateUtc" class="form-control" readonly />
    </div>
</div>