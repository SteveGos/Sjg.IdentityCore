@page
@model Sjg.IdentityCore.Areas.UserMgmt.Pages.AccAuthGroups.DeleteModel

@{
    ViewData["Title"] = "Delete Access Group";
}

<h5>@ViewData["Title"]</h5>

<hr />

<partial name="_AccAuthGroupHeader" for="AccAuthGroup" />

<div>

    <h5>Are you sure you want to delete this?</h5>

    <form method="post">
        <input type="hidden" asp-for="AccAuthGroup.AccAuthGroupId" />
        <input type="submit" value="Delete" class="btn btn-success" />
        <a asp-page="../AccAuthGroups/Details" asp-route-id="@Model.AccAuthGroup.AccAuthGroupId" class="btn btn-danger">Cancel</a>
    </form>
</div>